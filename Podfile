  inherit! :search_paths
  # Pods for testing
  pod 'RxBlocking', '6.6.0'
  pod 'RxTest', '6.6.0'
end

target 'JustNewsUITests' do
  inherit! :search_paths
  # Pods for testing
  pod 'RxBlocking', '6.6.0'
  pod 'RxTest', '6.6.0'
end
