# Comment the next line if you don't want to use dynamic frameworks
use_frameworks!

# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'JustNews' do
  pod 'RxSwift', '6.6.0'
  pod 'RxCocoa', '6.6.0'
  pod 'Swinject'
  pod 'SwiftLint'
  pod 'Kingfisher', '~> 8.0'
  pod 'SegueManager'
end

